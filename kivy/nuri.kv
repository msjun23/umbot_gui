FloatLayout:
    ################################################
    # Delivery #
    MDLabel:
        id: labelDelivery
        text: "Delivery"
        halign: "center"
        font_style: "H3"
        size_hint: 1, 1
        pos_hint: {"center_x": .13, "center_y": .92}

    ################################################
    # Room #
    MDLabel:
        id: labelRoom
        text: "Room           :"
        halign: "center"
        font_style: "H4"
        size_hint: 0.25, 0.1
        pos_hint: {"center_x": .15, "center_y": .82}

    MDTextField:
        id: textRoom
        hint_text: "Room"
        pos_hint: {"center_x": .4, "center_y": .81}
        size_hint: 0.20, 0.1
        icon_left: "key-variant"

    ################################################
    # Password #
    MDLabel:
        id: labelPassword
        text: "Password   :"
        halign: "center"
        font_style: "H4"
        size_hint: 0.25, 0.1
        pos_hint: {"center_x": .15, "center_y": .73}

    MDTextField:
        id: textPassword
        hint_text: "Password"
        pos_hint: {"center_x": .4, "center_y": .72}
        size_hint: 0.20, 0.1
        password: True
        icon_left: "key-variant"

    MDIconButton:
        icon: "eye-off"
        pos_hint: {"center_x": .525, "center_y": .72}
        theme_text_color: "Hint"
        on_release:
            self.icon = "eye" if self.icon == "eye-off" else "eye-off"
            textPassword.password = False if textPassword.password is True else True

    ################################################
    # Open #
    # Set  #
    MDRaisedButton:
        id: btnOpen
        text: "Open"
        size_hint: 0.235, 0.145
        pos_hint: {"center_x": .165, "center_y": .58}
        on_press: app.btnOpen_pressed(*args)

    MDRaisedButton:
        id: btnSet
        text: "Set"
        size_hint: 0.235, 0.145
        pos_hint: {"center_x": .435, "center_y": .58}
        on_press: app.btnSet_pressed(*args)

    ################################################
    # Disinfection #
    # Cleaning     #
    MDRaisedButton:
        id: btnDisinfection
        text: "Disinfection"
        size_hint: 0.235, 0.145
        pos_hint: {"center_x": .165, "center_y": .41}
        on_press: app.btnDisinfection_pressed(*args)

    MDRaisedButton:
        id: btnCleaning
        text: "Cleaning"
        size_hint: 0.235, 0.145
        pos_hint: {"center_x": .435, "center_y": .41}
        on_press: app.btnCleaning_pressed(*args)

    ################################################
    # STOP #
    MDRaisedButton:
        id: btnStop
        text: "STOP"
        size_hint: 0.47, 0.2905
        pos_hint: {"center_x": .30, "center_y": .17}
        on_press: app.btnStop_pressed(*args)

    ################################################
    # Map #
    Image:
        source: '/home/msjun-xavier/catkin_ws/src/umbot_gui/img/NURI.png'
        pos_hint: {"center_x": .78, "center_y": .5}
    
